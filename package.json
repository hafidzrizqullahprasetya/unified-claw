{
  "name": "unified-agentic-os",
  "version": "0.1.0",
  "description": "AI-powered unified commerce platform for UMKM Indonesia",
  "type": "module",
  "main": "dist/main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "tsx watch src/server-node.ts",
    "dev:frontend": "next dev",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "tsc && tsc --noEmit",
    "build:frontend": "next build",
    "start": "node dist/server-node.js",
    "start:frontend": "next start",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint .",
    "format": "prettier --write \"src/**/*.ts\"",
    "db:generate": "drizzle-kit generate:pg",
    "db:push": "drizzle-kit push:pg",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.43",
    "@ai-sdk/google": "^3.0.29",
    "@ai-sdk/openai": "^3.0.28",
    "@anthropic-ai/sdk": "^0.74.0",
    "@fullcalendar/core": "^6.1.19",
    "@fullcalendar/daygrid": "^6.1.19",
    "@fullcalendar/interaction": "^6.1.19",
    "@fullcalendar/list": "^6.1.19",
    "@fullcalendar/react": "^6.1.19",
    "@fullcalendar/timegrid": "^6.1.19",
    "@react-jvectormap/core": "^1.0.4",
    "@react-jvectormap/world": "^1.1.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.17",
    "@whiskeysockets/baileys": "^7.0.0-rc.9",
    "ai": "^6.0.85",
    "apexcharts": "^4.7.0",
    "autoprefixer": "^10.4.24",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.0",
    "drizzle-orm": "^0.30.0",
    "flatpickr": "^4.6.13",
    "hono": "^4.0.0",
    "jsonwebtoken": "^9.0.0",
    "midtrans-client": "^1.4.3",
    "next": "^16.1.6",
    "openai": "^6.21.0",
    "postgres": "^3.4.0",
    "react": "^19.2.4",
    "react-apexcharts": "^1.8.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.2.4",
    "react-dropzone": "^14.3.8",
    "swiper": "^11.2.10",
    "tailwind-merge": "^2.6.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@svgr/webpack": "^8.1.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.19.25",
    "@types/react": "^19.2.1",
    "@types/react-dom": "^19.2.1",
    "@types/react-transition-group": "^4.4.12",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitest/coverage-v8": "^1.1.0",
    "autoprefixer": "^10.4.24",
    "concurrently": "^9.1.0",
    "drizzle-kit": "^0.20.0",
    "eslint": "^9.39.1",
    "eslint-config-next": "16.0.7",
    "postcss": "^8.5.6",
    "prettier": "^3.1.0",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^1.1.0"
  },
  "overrides": {
    "@react-jvectormap/core": {
      "react": "^16.8.0 || ^17 || ^18 || ^19",
      "react-dom": "^16.8.0 || ^17 || ^18 || ^19"
    },
    "@react-jvectormap/world": {
      "react": "^16.8.0 || ^17 || ^18 || ^19",
      "react-dom": "^16.8.0 || ^17 || ^18 || ^19"
    }
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
